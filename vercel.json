
{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_GOOGLE_CLIENT_ID": "@google_client_id"
  },
  "buildCommand": "pnpm install --frozen-lockfile && pnpm build"
}
    